{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Parameters": {
        "authunderdogAuthUserPoolId": {
            "Type": "String",
            "Default": "authunderdogAuthUserPoolId"
        },
        
        "authunderdogAuthAppClientID": {
            "Type": "String",
            "Default": "authunderdogAuthAppClientID"
        },
        "authunderdogAuthAppClientIDWeb": {
            "Type": "String",
            "Default": "authunderdogAuthAppClientIDWeb"
        },
        "AuthRoleArn": {
            "Type": "String"
        },
        "UnauthRoleArn": {
            "Type": "String"
        },
        "env": {
            "Type": "String"
        }
    },
    "Conditions": {
        "ShouldNotCreateEnvResources": {
            "Fn::Equals": [
                {
                    "Ref": "env"
                },
                "NONE"
            ]
        }
    },
    "Resources": {
    
        "newBettorGroup": {
          "Type" : "AWS::Cognito::UserPoolGroup",
          "Properties" : {
              "GroupName" : "newBettor",
              "Precedence" : 1,
              
              "UserPoolId" : {
                "Ref": "authunderdogAuthUserPoolId"
              }
            }
        },
        
    
        "sharpPlayerGroup": {
          "Type" : "AWS::Cognito::UserPoolGroup",
          "Properties" : {
              "GroupName" : "sharpPlayer",
              "Precedence" : 2,
              
              "UserPoolId" : {
                "Ref": "authunderdogAuthUserPoolId"
              }
            }
        },
        
    
        "highRollerGroup": {
          "Type" : "AWS::Cognito::UserPoolGroup",
          "Properties" : {
              "GroupName" : "highRoller",
              "Precedence" : 3,
              
              "UserPoolId" : {
                "Ref": "authunderdogAuthUserPoolId"
              }
            }
        },
        
    
        "adminGroup": {
          "Type" : "AWS::Cognito::UserPoolGroup",
          "Properties" : {
              "GroupName" : "admin",
              "Precedence" : 4,
              
              "UserPoolId" : {
                "Ref": "authunderdogAuthUserPoolId"
              }
            }
        },
        
    
    
    },
    
}
